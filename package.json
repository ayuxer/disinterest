{
  "name": "disinterest",
  "version": "1.0.0",
  "author": "Ayuka <ayuxified@kittens.cat>",
  "description": "Privacy-respecting, NoJS-supporting Pinterest frontend",
  "main": "dist/index.mjs",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "build:client": "vite build --outDir dist/client --ssrManifest && vite build --outDir dist/server --ssr /index.js",
    "build:server": "node build.mjs",
    "build": "pnpm build:client && pnpm build:server",
    "dev": "pnpm build:server && node . --dev",
    "start": "pnpm build && node .",
    "fmt": "prettier . --write"
  },
  "dependencies": {
    "@fastify/one-line-logger": "^1.4.0",
    "@fastify/sensible": "^5.6.0",
    "@fastify/vite": "^6.0.6",
    "@fastify/vue": "^0.5.0",
    "cheerio": "1.0.0-rc.12",
    "devalue": "^5.0.0",
    "fastify": "^4.28.0",
    "unihead": "^0.0.7",
    "vue": "^3.4.29",
    "vue-router": "^4.3.3"
  },
  "devDependencies": {
    "@types/node": "^20.14.7",
    "@vitejs/plugin-vue": "^5.0.5",
    "autoprefixer": "^10.4.19",
    "esbuild": "^0.21.5",
    "postcss": "^8.4.38",
    "postcss-nesting": "^12.1.5",
    "postcss-preset-env": "^9.5.14",
    "prettier": "^3.3.2",
    "tailwindcss": "^3.4.4",
    "typescript": "^5.5.2",
    "vite": "^5.3.1",
    "vue-tsc": "^2.0.21"
  }
}
